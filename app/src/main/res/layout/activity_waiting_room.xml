<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="289dp"
        android:layout_height="78dp"
        android:text="Open nu de Teams vergadering op jouw desktop/laptop voordat de intervisie begint en zorg ervoor dat de verrbinding technisch werkt!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/tv_groupname_waiting_room"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Group: [groepnaam]"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.331" />

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="140dp"
        android:layout_gravity="center"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">

        <TextView
            android:id="@+id/user_and_icon_1_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="#9A2424"
            android:maxWidth="72dp"
            android:maxHeight="90dp"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/ic_launcher_background"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

        <TextView
            android:id="@+id/user_and_icon_2_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="@android:color/transparent"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/yellow_100"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

        <TextView
            android:id="@+id/user_and_icon_3_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="@android:color/transparent"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/yellow_100"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="410dp"
        android:layout_height="134dp"
        android:layout_gravity="center"
        android:background="@drawable/button_roundcorner"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.722">

        <TextView
            android:id="@+id/user_and_icon_4_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="@android:color/transparent"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/yellow_100"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

        <TextView
            android:id="@+id/user_and_icon_5_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="@android:color/transparent"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/yellow_100"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

        <TextView
            android:id="@+id/user_and_icon_6_group_preview"
            android:layout_width="72dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_roundcorner"
            android:backgroundTint="@android:color/transparent"
            android:text="@string/name_user_group_assembly"
            android:textAlignment="center"
            android:textColor="@color/yellow_100"
            app:drawableTopCompat="@drawable/temp_profile_picture_72x72" />

    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_start_waiting_room"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:background="@drawable/button_roundcorner"
        android:backgroundTint="@color/blue_100"
        android:text="Start"
        android:textColor="@color/light_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.829" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_afzeggen_waiting_room"
        android:layout_width="260dp"
        android:layout_height="47dp"
        android:background="@drawable/button_roundcorner"
        android:backgroundTint="@color/red_100"
        android:text="Afzeggen"
        android:textColor="@color/light_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93" />

</androidx.constraintlayout.widget.ConstraintLayout>